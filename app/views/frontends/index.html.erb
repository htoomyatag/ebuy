<p id="notice"><%= notice %></p>

<h1>Listing Frontends</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @frontends.each do |frontend| %>
      <tr>
        <td><%= link_to 'Show', frontend %></td>
        <td><%= link_to 'Edit', edit_frontend_path(frontend) %></td>
        <td><%= link_to 'Destroy', frontend, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Frontend', new_frontend_path %>
