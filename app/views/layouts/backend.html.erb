<!DOCTYPE html>
<html>
<head>
  <title>eBuymyanmar</title>
  <%= stylesheet_link_tag  'bootstrap.min.css' %>
  <%= stylesheet_link_tag  'backend.css' %>
  <%= stylesheet_link_tag  'jquery-ui.css' %>
  <%= stylesheet_link_tag  'quill.snow.css' %>

  <%= javascript_include_tag 'jquery.min.js' %>
  <%= javascript_include_tag 'jquery_ujs.js' %>
  <%= javascript_include_tag 'jquery-ui.js' %>
  <%= javascript_include_tag 'bootstrap.min.js' %>
  <%= javascript_include_tag 'bootstrap-submenu.min.js' %>

  <%= csrf_meta_tags %>

  </head>


   <% if admin_signed_in? %>
          <nav class="navbar navbar-default">
            <div class="container-fluid">
              <!-- Brand and toggle get grouped for better mobile display -->
              <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                  <span class="sr-only">Toggle navigation</span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                  <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">EBuy</a>
              </div>

              <!-- Collect the nav links, forms, and other content for toggling -->
              <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
          
            
              <ul class="nav navbar-nav navbar-right">
                <li class="dropdown">
                
                  <a href="#" class="dropdown-toggle" data-toggle="dropdown">Account Setting<b class="caret"></b></a>
                  <ul class="dropdown-menu">
                    <li><%= link_to "Account Setting", edit_admin_registration_path  %></li>
                    <li><a href="#">Help</a></li>
                    <li class="divider"></li>
                    <li>   <%= link_to "Log Out", destroy_admin_session_path, method: :delete  %></li>
                  </ul>
               
                </li>
              </ul>



              </div><!-- /.navbar-collapse -->
            </div><!-- /.container-fluid -->
          </nav>


           <body>
              <div class="rows">
                  <div class="col-md-2">
                  
                  
                        <ul class="list-group">

                          <li class="list-group-item">
                             <%= link_to 'Dashboard', dashboard_path %>
                          </li>
                           <li class="list-group-item">
                            <%= link_to 'Product listing', products_path %>
                          </li>
                          <li class="list-group-item">
                            <%= link_to 'Order listing', orders_path %>
                          </li>

                          <li class="list-group-item">
                            <%= link_to 'Coupon listing', coupons_path %>
                          </li>

                          <li class="list-group-item">
                            <%= link_to 'Currency Exchange', currency_exchanges_path %>
                          </li>


                           <li class="list-group-item">
                            <%= link_to 'Buyer listing', buyer_list_path %>
                          </li>
                        
                          <li class="list-group-item">
                            <%= link_to "Account Setting", edit_admin_registration_path  %>
                          </li>
                          <li class="list-group-item">
                             <%= link_to "Log Out", destroy_admin_session_path, method: :delete  %>
                          </li>

                        </ul>

                  </div>
                  <div class="col-md-10">
                    <%= yield %>
                  </div>
              </div>
          </body>




     <% else %>

                <%= yield %>

     <% end %>
     

 



		


</html>
